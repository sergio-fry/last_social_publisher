<p id="notice"><%= notice %></p>

<h1>Listing Channels</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Publisher klass</th>
      <th>Credentials</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @channels.each do |channel| %>
      <tr>
        <td><%= channel.title %></td>
        <td><%= channel.publisher_klass %></td>
        <td><%= channel.credentials %></td>
        <td><%= link_to 'Show', channel %></td>
        <td><%= link_to 'Edit', edit_channel_path(channel) %></td>
        <td><%= link_to 'Destroy', channel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Channel', new_channel_path %>
